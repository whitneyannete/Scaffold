    "tailwindcss": "^3.2.7"
  },
  "dependencies": {
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.7",
    "live-server": "^1.1.0",
    "postcss-cli": "^9.1.0"
  }
